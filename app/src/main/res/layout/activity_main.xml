<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/img"
    tools:context=".MainActivity"
    tools:layout_editor_absoluteX="-3dp"
    tools:layout_editor_absoluteY="62dp">

    <EditText
        android:id="@+id/telefon"
        android:layout_width="194dp"
        android:layout_height="45dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="316dp"
        android:backgroundTint="#009E9696"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="telefon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/email" />

    <EditText
        android:id="@+id/Name"
        android:layout_width="194dp"
        android:layout_height="45dp"
        android:layout_marginEnd="204dp"
        android:layout_marginBottom="544dp"
        android:backgroundTint="#009E9696"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/ves"
        android:layout_width="194dp"
        android:layout_height="45dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="372dp"
        android:backgroundTint="#009E9696"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="ves"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/rost" />

    <EditText
        android:id="@+id/email"
        android:layout_width="194dp"
        android:layout_height="45dp"
        android:layout_marginEnd="204dp"
        android:layout_marginBottom="208dp"
        android:backgroundTint="#009E9696"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="email"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rost"
        app:layout_constraintVertical_bias="0.092" />

    <EditText
        android:id="@+id/rost"
        android:layout_width="194dp"
        android:layout_height="45dp"
        android:layout_marginEnd="204dp"
        android:layout_marginBottom="272dp"
        android:backgroundTint="#009E9696"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="rost"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pol"
        app:layout_constraintVertical_bias="0.145" />

    <EditText
        android:id="@+id/pol"
        android:layout_width="194dp"
        android:layout_height="45dp"
        android:layout_marginEnd="204dp"
        android:layout_marginBottom="340dp"
        android:backgroundTint="#009E9696"

        android:ems="10"
        android:inputType="textPersonName"
        android:text="pol"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/age"
        app:layout_constraintVertical_bias="0.09" />

    <EditText
        android:id="@+id/age"
        android:layout_width="194dp"
        android:layout_height="45dp"
        android:layout_marginEnd="204dp"
        android:layout_marginBottom="404dp"
        android:ems="10"
        android:backgroundTint="#009E9696"
        android:inputType="textPersonName"
        android:text="age"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Name"
        app:layout_constraintVertical_bias="0.111" />

    <Button
        android:id="@+id/button"
        android:layout_width="389dp"
        android:layout_height="101dp"
        android:backgroundTint="#0FA003"
        android:text="Заболевания пользователя"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="434dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="196dp"
        android:layout_height="174dp"
        app:srcCompat="@drawable/img_1"
        tools:layout_editor_absoluteX="213dp"
        tools:layout_editor_absoluteY="140dp" />
</androidx.constraintlayout.widget.ConstraintLayout>